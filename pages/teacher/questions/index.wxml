<view class="weui-cells">
    <block
        wx:for="{{questions}}"
        wx:key="{{item.time}}">
        <view>{{item.time}}</view>

        <block
            wx:for="{{item.qts}}"
            wx:for-item="qt"
            wx:key="{{qt.id}}">

            <navigator
                class="weui-cell"
                url="/pages/teacher/students/index">
                <block wx:if="{{qt.type == 'text'}}">
                    {{qt.question}}
                </block>

                <block wx:else>
                    <image
                    style="width:320rpx;height:180rpx;"
                    src="{{qt.question}}" />
                </block>
            </navigator>
        </block>

    </block>
</view>